{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": ["plugin:prettier/recommended"],
    "plugins": ["eslint-plugin-react"],
    "rules": {
        "semi": "error"
    }
}
