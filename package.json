{
  "name": "boilerplate-react-webpack",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "webpack && knex migrate:latest",
    "dev": "nodemon --watch server server/index & webpack --watch",
    "start": "node server/index",
    "test": "jest",
    "h:deploy": "git push heroku master",
    "h:cliff": "git push heroku deploy:master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex"

  },
  "nodemonConfig": {
    "ignore": ["node_modules/*"]
  },
  "jest": {
    "collectCoverage": true
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "enzyme": "^2.8.2",
    "nodemon": "^1.11.0",

    "sqlite3": "^4.0.6",
    "supertest": "^3.0.0",
    "jest": "^23.4.1"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.15.2",
    "bulma": "^0.7.1",
    "cors": "^2.8.3",
    "css-loader": "^1.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "font-awesome": "^4.7.0",
    "jsdom": "^11.0.0",
    "jsonwebtoken": "^7.4.3",
    "jsx-loader": "^0.13.2",
    "jwt-decode": "^2.2.0",
    "knex": "^0.13.0",
    "nock": "^9.4.2",
    "node-sass": "^4.9.2",
    "passport": "^0.4.0",
    "pg": "^7.4.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "react-tippy": "^1.2.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.21.0",
    "superagent": "^2.2.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
